{
  "extends": [
    "@voxpelli:vpBase",
    ":semanticCommits"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "extends": [
      "@voxpelli:vpSchedule"
    ]
  },
  "packageRules": [
    {
      "matchDepTypes": [
        "dependencies"
      ],
      "semanticCommitType": "fix"
    },
    {
      "matchDepTypes": [
        "peerDependencies"
      ],
      "rangeStrategy": "bump",
      "semanticCommitScope": "",
      "semanticCommitType": "fix!"
    }
  ]
}
